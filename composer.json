{
    "name": "arifurrahmansw/laravel-repository",
    "description": "A Laravel package that provides a reusable and extendable repository pattern implementation.",
    "keywords": ["laravel", "repository", "repository-pattern"],
    "homepage": "https://github.com/arifurrahmansw/laravel-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Arifur Rahman",
            "email": "arifurrahman91@live.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3 || ^8.4",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "ArifurRahmanSw\\Repository\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArifurRahmanSw\\Repository\\RepositoryServiceProvider"
            ],
            "aliases": {
                "CommonRepo": "App\\Facades\\CommonRepo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
